{
  "name": "animate.css",
  "version": "3.7.2",
  "main": "animate.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/daneden/animate.css.git"
  },
  "author": {
    "name": "Daniel Eden"
  },
  "homepage": "https://daneden.github.io/animate.css/",
  "license": "MIT",
  "animateConfig": {
    "prefix": "animate__"
  },
  "scripts": {
    "start": "npm-run-all raw prod compat",
    "compat": "npx postcss source/animate.css -o animate.compat.css --no-map --env compat",
    "dev": "npx postcss source/animate.css -o animate.css --no-map --env development -w",
    "raw": "npx postcss source/animate.css -o animate.css --no-map --env development",
    "prod": "npx postcss source/animate.css -o animate.min.css --no-map --env production",
    "format": "prettier --write \"**/*.{js,json,md,css}\"",
    "precommit": "lint-staged"
  },
  "browserslist": [
    "> 3%",
    "last 2 versions"
  ],
  "style": "./animate.css",
  "jspm": {
    "main": "animate.css!",
    "format": "global",
    "directories": {
      "lib": "./"
    }
  },
  "devDependencies": {
    "autoprefixer": "^9.6.0",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.17",
    "postcss-cli": "^6.1.2",
    "postcss-header": "^1.0.0",
    "postcss-import": "^12.0.1",
    "postcss-prefixer": "^2.1.1",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.18.2"
  },
  "lint-staged": {
    "*.{js,json,md,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run start"
    }
  }
}
